@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Книжный магазин</title>
</head>
<body>
    <div>
        <h3>Распродажа книг</h3>
        <table>
            <tr>
                <td><p>Название книги</p></td>
                <td><p>Автор</p></td>
                <td><p>Цена</p></td>
                <td></td>
            </tr>
            @foreach (var b in ViewBag.Tarif)
            {
            <tr>
                <td><p>@b.Id</p></td>
                <td><p>@b.Guid</p></td>
                <td><p>@b.Name</p></td>
                <td><p>@b.Type</p></td>
                <td><p>@b.Groupe</p></td>
                
                <td><p>@b.Article</p></td>
                
            </tr>
            }
        </table>
    </div>
</body>
</html>